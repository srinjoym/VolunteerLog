<div class = "container">
  <h1>Past Logs</h1>
  <table class="table table-hover">
    <tr>
      <th>Date</th>
      <th>Time Started</th>
      <th>Time Ended</th>
      <th>Reason</th>
    </tr>
    <% @visitor_logs.each do |visitor_log| %>
    <tr>
      <td><%=visitor_log.time_in.strftime("%m-%d-%Y ")%></td>
      <td><%=visitor_log.time_in.strftime("%H:%M ")%></td>
        <td><%=visitor_log.time_out.strftime("%H:%M ")%></td>
      <td><%=visitor_log.reason%></td>
    </tr>
    <%end%>
  </table>
  <%= will_paginate %>
</div>
